{
    "compilerOptions": {
			"target": "es6",
			"module": "commonjs",
			"outDir": "./lib",
			"allowJs": true,
			"moduleResolution": "node",
			"allowSyntheticDefaultImports": true,
			"skipLibCheck": true
		},
		"include": ["src/**/*.ts", "src/client/**.js"],
		"exclude": ["node_modules/**/*.ts"]
}